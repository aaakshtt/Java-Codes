//
package com.company;

public class ContinueLoop {
    public static void main(String[] args) {
        for(int i = 0; i<5; i++ ){
            System.out.println("Java is great");
            System.out.println(i);
            if(i==2){
                System.out.println("The number above is 2");
                continue;
            }
        }
    }
}
