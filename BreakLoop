package com.company;

public class BreakLoop {
    public static void main(String[] args) {
        for(int i = 0; i<5; i++ ){
            System.out.println("Java is great");
            System.out.println(i);
            if(i==4){
                System.out.println("Ending Loop");
                break;
            }
        }
    }
}
