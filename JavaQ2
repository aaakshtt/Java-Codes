//Write a program to calculate the sum of first 10 natural number using Loop
package com.company;

import java.util.Scanner;

public class LoopsQuestionPractice {
    public static void main(String[] args) {
        int sum = 0;
        for (int i = 1;i<11;i++){
            sum += i;

        }
        System.out.println("sum is  " + sum);


    }
}
