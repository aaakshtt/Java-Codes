
class A {                 //Parent class
  int a  =10;           
}
  
public class B extends A{               //Sub class
  int a =20;
  public void show(int a){
    System.out.println(a);           //Will print the value given in the object called
    System.out.println(this.a);			//Will refer to the current class variables
    System.out.println(super.a);		//Will refer to the parent class variables
  }
  
  public static void main(String[] args) {
    B ob1 = new B();
    ob1.show(30);          //Will cal line 9
    
    
  }
}
  
  
